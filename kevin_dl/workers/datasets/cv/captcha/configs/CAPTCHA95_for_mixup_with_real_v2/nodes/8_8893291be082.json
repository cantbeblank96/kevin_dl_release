{
    "width": 64,
    "height": 64,
    "font_size": 51,
    "color_options_of_text": null,
    "color_of_bg": 255,
    "max_offset_of_text": "<eval>(0.5, 0.5)",
    "b_use_transforms": true,
    "transforms": [
        {
            "name": ":for_images:torchvision:ToTensor",
            "paras": {}
        },
        {
            "name": ":for_images:torchvision:RandomAffine",
            "paras": {
                "degrees": 30,
                "translate": "<eval>(0.05, 0.1)",
                "shear": 30,
                "fill": "<eval>(1.0, 1.0, 1.0)"
            }
        },
        {
            "name": ":for_images:torchvision:GaussianBlur",
            "paras": {
                "kernel_size": 3,
                "sigma": "<eval>(0.1, 1.5)"
            }
        },
        {
            "name": ":for_images:torchvision:ColorJitter",
            "paras": {
                "brightness": 0.3,
                "contrast": 0.3,
                "saturation": 0.3
            }
        }
    ],
    "b_add_noise_line": true,
    "color_options_of_line": "<eval>('gray',)",
    "b_add_noise_point": true
}